/login:
  description: Inloggen van een Loyalis klant
  post:
    description: Inloggen van een Loyalis klant
    body:
      application/json:
        type: InlogGegevens
        example: |
          {
            "gebruikersnaam": "gebruiker.van.de.test@loyalis.nl",
            "wachtwoord": "LOY1234!"
          }
    responses:
      200:
        body:
          application/json:
            type: LoginTokenResponse
            example: |
              {
                "token_type": "bearer",
                "access_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJrbGFudG51bW1lciI6MTU5MDk2NiwiZ2VicnVpa2Vyc25hYW0iOiJMYXVyYSIsImFjY291bnRzdGF0dXMiOiJBY3RpZWYiLCJleHBpcmUiOjIzMzYzMzYzNjZ9.iwoyaECWrB-JHcFZKPn-VyRUH06fimbWFbL2rAVvQnw"
              }
      400:
        description: Input parameters zijn fout
      401:
        description: Inloggegevens zijn incorrect
      503:
        description: Algemene serverfout

/klanten/{klantnummer}/persoonsgegevens:
  description: PersoonsGegevens van een Loyalis klant
  uriParameters:
    klantnummer:
      type: Klantnummer
      required: true
  get:
    headers:
      Authorization:
        required: true
        type: Authorizationheader
    responses:
      200:
        description: PersoonsGegevens van een Loyalis klant
        body:
          application/json:
            type: PersoonsGegevens
            example: |
              {
                "klantnummer": 123456789,
                "aanhef": "mevrouw Test",
                "voornaam": "Gebruiker",
                "achternaam": "Test",
                "geboortedatum": "1967-01-14T00:00:00Z",
                "telefoonnummer": "0455799222",
                "mobielnummer": "0612345678",
                "emailadres": "gebruiker.van.de.test@loyalis.nl",
                "heeftMachtiging": true
              }
      400:
        description: Input parameters zijn fout
      401:
        description: Verificatie van de aanmeldgegevens is mislukt
      403:
        description: Het uitvoeren van deze actie is niet toegestaan
      503:
        description: Algemene serverfout
  patch:
      description: PersoonsGegevens bijwerken van een Loyalis klant
      headers:
        Authorization:
          required: true
          type: Authorizationheader
